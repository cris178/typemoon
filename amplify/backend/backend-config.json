{
	"api": {
		"typemoon": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"authConfig": {
					"additionalAuthenticationProviders": [],
					"defaultAuthentication": {
						"authenticationType": "API_KEY",
						"apiKeyConfig": {
							"description": "Type Moons Social API",
							"apiKeyExpirationDays": "365"
						}
					}
				}
			}
		}
	},
	"hosting": {
		"amplifyhosting": {
			"service": "amplifyhosting",
			"type": "cicd"
		}
	}
}